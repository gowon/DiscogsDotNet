{
  "$schema": "https://json.schemastore.org/appsettings.json",
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient.IDiscogsClientV2": "Information"
    },
    "NLog": {
      "IncludeScopes": true,
      "RemoveLoggerFactoryFilter": false
    }
  },
  "DiscogsClientV2": {
    "UserAgent": "DiscogsDotNet",
    "UserAgentVersion": "1.0",
    "UserAgentUri": "https://github.com/gowon/DiscogsDotNet"
  }
}